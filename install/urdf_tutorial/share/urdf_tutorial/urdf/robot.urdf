<?xml version="1.0"?>
<robot name="robot">

  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.25 0.20 1"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.20 1"/>
      </geometry>
    </collision>
  </link>

  <link name="Trans_Z_Axes">
    <visual>
      <geometry>
        <box size="0.25 0.20 1"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.20 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="Z_Translation" type="prismatic">
    <parent link="base_link"/>
    <child link="Trans_Z_Axes"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="0" upper="0.550" velocity="0.5"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="transX_Axes">
    <visual>
      <geometry>
        <box size="0.8 0.2 0.05"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.8 0.2 0.05"/>
      </geometry>
    </collision>
  </link>

  <joint name="X_Translation" type="prismatic">
    <parent link="Trans_Z_Axes"/>
    <child link="transX_Axes"/>
    <limit effort="1000.0" lower="0" upper="0.550" velocity="0.5"/>
    <origin xyz="-0.275 0 0.525"/>
  </joint>

  <link name="transY_Base">
    <visual>
      <geometry>
        <box size="0.05 1.20 0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 1.20 0.05"/>
      </geometry>
    </collision>

  </link>
  
  <joint name="TransY_Fixation" type="fixed">
    <parent link="transX_Axes"/>
    <child link="transY_Base"/>
    <origin xyz="0.425 0 0"/>
  </joint>

  <link name="transY_Box">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>

  </link>

  <joint name="Y_Translation" type="prismatic">
    <parent link="transY_Base"/>
    <child link="transY_Box"/>
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="0" upper="1.15" velocity="0.5"/>
    <origin xyz="0 -0.575 0.05"/>
  </joint>

  <link name="Rotation_Part">
    <visual>
      <geometry>
        <box size="0.035 0.035 0.035"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.035 0.035 0.035"/>
      </geometry>
    </collision>

  </link>
  
  <joint name="Rotation" type="continuous">
    <parent link="transY_Box"/>
    <child link="Rotation_Part"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0.04 0 0"/>
  </joint>

    <joint name="Rotation" type="continuous">
    <parent link="transY_Box"/>
    <child link="Rotation_Part"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.04 0 0"/>
  </joint>

</robot>

